.wrapper-top {
  display: flex;
  justify-content: space-between;
  padding-left: 10rpx;
  padding-right: 10rpx;
  background-color: #f7f7f7;
  height: 80rpx;
  line-height: 80rpx;
}
.wrapper-top text{
font-size: 30rpx;
}
.wrapper-head {
  display: flex;
  justify-content: space-around;
}

.wrapper-head text {
  padding-top: 45rpx;
}

.wrapper-head image {
  padding-top: 20rpx;
  height: 100rpx;
  width: 100rpx;
}

.wrapper-top-text {
  color: #5D565B;
  font-size: 29rpx;
}

.wrapper-head {
  height: 150rpx;
  background-color: #6eacb7;
  display: flex;
}

.wrapper-head text {
  color: #d3e6e9;
  font-size: 35rpx;
}

.wrapper-botton {
  border: 2rpx #f7f7f7 solid;
  height: 40rpx;
  border-radius: 20rpx;
  width: 120rpx;
  background-color: white;
  text-align: center;
  margin-right: 50rpx;
  margin-top: 45rpx;
  font-size: 25rpx;
  line-height: 40rpx;
}

.wrapper-scorral image {
  width: 100%;
  height: 125rpx;
}

.scorral-image {
  display: flex;
}

.scorral-image {
  width: 360rpx;
  height: 400rpx;
}

.scorral-content {
  display: flex;
  flex-direction: column;
  margin-left: 10rpx;
}

.scorral-content image {
  height: 360rpx;
  width: 360rpx;
}

.scorral-price {
  color: #fb9958;
}

.scorral-text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 360rpx;
  font-size: 33rpx;
  color: #4b4449;
}
